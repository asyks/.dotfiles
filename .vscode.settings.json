{
  "editor.minimap.enabled": false,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "files.exclude": {
    ".vscode/": true,
    ".mypy_cache/": true
  },
  "files.insertFinalNewline": true,
  "C_Cpp.default.includePath": ["${workspaceFolder}/**"],
  "C_Cpp.default.defines": [],
  "C_Cpp.default.compilerPath": "/bin/gcc",
  "C_Cpp.default.cStandard": "gnu17",
  "C_Cpp.default.cppStandard": "gnu++14",
  "C_Cpp.default.intelliSenseMode": "gcc-x64",
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "json.format.enable": true,
  "[json]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": true },
    { "language": "typescriptreact", "autoFix": true }
  ],
  "python.linting.enabled": true,
  "python.testing.pytestEnabled": true,
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "python.linting.pylintPath": "${env:HOME}/.pyenv/shims/pylint",
  "python.linting.flake8Enabled": true,
  "python.linting.lintOnSave": true,
  "python.linting.pycodestyleEnabled": false,
  "python.linting.pydocstyleEnabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.mypyPath": "${env:HOME}/.pyenv/shims/mypy",
  "python.linting.mypyArgs": ["--show-error-codes"],
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "${env:HOME}/.pyenv/shims/black",
  "python.defaultInterpreterPath": "${env:HOME}/.pyenv/shims/python",
  "[python]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.rulers": [88],
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    },
    "editor.formatOnType": true
  },
  "githubPullRequests.queries": [
    {
      "label": "Waiting For My Review",
      "query": "is:open review-requested:${user}"
    },
    {
      "label": "Created By Me",
      "query": "is:open author:${user}"
    },
    {
      "label": "web-py",
      "query": "is:open label:web-py review:required -reviewed-by:${user}"
    },
    {
      "label": "web-js",
      "query": "is:open label:web-js review:required -reviewed-by:${user}"
    },
    {
      "label": "Already reviewed",
      "query": "is:open reviewed-by:${user}"
    }
  ],
  "scm.diffDecorations": "none",
  "vim.hlsearch": true,
  "vim.autoindent": false,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["c-r"],
      "commands": ["redo"]
    }
  ],
  "window.title": "${activeEditorLong}${separator}${rootName}",
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "keyboard.touchbar.enabled": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.cursorStyle": "line",
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "redhat.telemetry.enabled": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "Default Dark+",
  "editor.formatOnSave": true
}
